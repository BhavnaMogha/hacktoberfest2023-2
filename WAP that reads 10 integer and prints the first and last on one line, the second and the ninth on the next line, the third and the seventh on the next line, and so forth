#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
	int num1,num2,num3,num4,num5,num6,num7,num8,num9,num10;
	cout<<"enter the ten numbers:";
	cin>>num1>>num2>>num3>>num4>>num5>>num6>>num7>>num8>>num9>>num10;
	cout<<"\nyour numbers are:\n"<<endl;
	cout<<setw(2)<<num1<<"\t"<<setw(2)<<num10<<"\n";
	cout<<setw(2)<<num2<<"\t"<<setw(2)<<num9<<"\n";
	cout<<setw(2)<<num3<<"\t"<<setw(2)<<num8<<"\n";
	cout<<setw(2)<<num4<<"\t"<<setw(2)<<num7<<"\n";
	cout<<setw(2)<<num5<<"\t"<<setw(2)<<num6<<"\n";
	return 0;
}
